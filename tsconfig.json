{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,

    "noImplicitAny": true,
    "strictNullChecks": true,

    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "node",

    "esModuleInterop": true,

    "resolveJsonModule": true,

    "jsx": "react-native",

    "paths": {
      "#/*": ["./src/*"]
    }
  }
}
